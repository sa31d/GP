@model IEnumerable<PrintNow.Models.Product_Has_Material>

@{
    ViewBag.Title = "ProductDetails";
}

<h2>ProductDetails</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Material.materialType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product.productType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @Html.ActionLink(item.Material.materialName, "RequestProduct", new { id = item.Product.productID,material=item.Material.materialName })
        </td>
        
       
    </tr>
}

</table>
